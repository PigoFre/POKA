{
  "testRegex": "(/test/.*|(\\.|/))\\.spec\\.js?$",
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "moduleNameMapper": {
    "^.+\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
  },
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "lcov"
  ]
}
